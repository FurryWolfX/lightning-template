{
  "compilerOptions": {
    "experimentalDecorators": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es2017",
    "allowJs": true,
    "sourceMap": true,
    "strict": true,
    "removeComments": true,
    "outDir": "./build",
    "baseUrl": "./src",
    "types": [
      "@types/node",
      "@wolfx/x-sql/types"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
